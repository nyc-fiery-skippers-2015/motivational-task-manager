
<h1> User profiles </h1>
<div class="container">
  <p>Author: <%= user.name %></p>
</div>
<% user.lists.each do |l| %>
  <p> List title:<%= l.title %> </p>
  <p> URL: <a href="/users/<%= user.id %>/<%= l.url %>"><%= l.title %></a> </p>
  <p> Public URL: /users/<%= user.id %>/<%= l.url %></p>
<% end %>
